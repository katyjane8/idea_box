<div class="form">
  <div class="field-wrap">
    <%= form_for [:admin, @image] do |f| %>
      <%= f.label :image, "Attach an Image" %><br>
      <%= f.file_field :image %><br>
      <%= f.submit class: "sub-btn" %>
      <%= link_to "Back to Admin Dashboard", admin_dashboard_index_path %>
    <% end %>
  </div>
</div>

<!DOCTYPE html>
<html>
  <head>
    <title>IdeaBox</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class="nav">
      <li> <%= link_to 'Log Out', logout_path, method: :delete %> </li>
    </div>
    <p class="flash">
      <%= flash.notice %>
    </p>
    <%= yield %>
  </body>
</html>
